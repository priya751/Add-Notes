<div class="form-group">
    <label for="title">Title</label>
    <input value="<%= note.title %>" type="text" name="title" id="title" class="form-control rounded-0" required />
</div>
<div class="form-group">
    <label for="description">Description</label>
    <textarea name="description" id="description" class="form-control rounded-0" required><%= note.description %>
 </textarea>
</div>
<a href="/" class="btn btn-secondary rounded-0">Cancel</a>
<button type="submit" class="btn btn-primary rounded-0">Add</button>